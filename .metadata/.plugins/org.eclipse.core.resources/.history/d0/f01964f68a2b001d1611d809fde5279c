package br.com.poo.sistemabancario.principal;

import br.com.poo.sistemabancario.contas.Conta;

public class Principal {

	public static void main(String[] args) {
		Conta minhaConta = new Conta();
		
		minhaConta.titular = "Priscila";
		minhaConta.saldo = 1000.0;
		minhaConta.numero = 132;
		minhaConta.sacar(50.0);
		minhaConta.depositar(20.0);
		
		System.out.println("Saldo atual: " + minhaConta.saldo
				+ "\nNome do titular: " + minhaConta.titular
				+ "\nNÃºmero da conta: " + minhaConta.numero);
	}

}
